@model ConfigurationModel
       .beautyShop-primary-color-bg {
        background-color: @String.Format("{0}{1}", Model.PrimaryThemeColor, " !important");
        }

        .beautyShop-primary-color-border {
        border-color: @String.Format("{0}{1}", Model.PrimaryThemeColor, " !important");
        }

        .beautyShop-primary-color-hover:hover {
        color: @String.Format("{0}{1}", Model.PrimaryThemeColor, " !important");
        }

        .beautyShop-primary-color-bg-hover:hover {
        background-color: @String.Format("{0}{1}", Model.PrimaryThemeColor, " !important");
        }

        .beautyShop-header-color {
        background-color: @String.Format("{0}{1}", Model.HeaderColor, " !important");
        }
        .beautyShop-header-font-color {
         color: @String.Format("{0}{1}", Model.HeaderFontColor, " !important");
        }
        .beautyShop-secondery-color-bg {
        background-color: @String.Format("{0}{1}", Model.SecondaryThemeColor, " !important");
        }

        .beautyShop-secondery-color-text {
        color: @String.Format("{0}{1}", Model.SecondaryThemeColor, " !important");
        }

        .beautyShop-secondery-color-border {
        border-color: @String.Format("{0}{1}", Model.SecondaryThemeColor, " !important");
        }

        .beautyShop-paragraph-text {
        font-size: @String.Format("{0}{1}", Model.ParagraphTextSize, "px !important");
        }

        .beautyShop-breadcrumb-title {
        font-size: @String.Format("{0}{1}", Model.BreadcrumbTitleSize, "px !important");
        }

        .beautyShop-breadcrumb-text {
        font-size: @String.Format("{0}{1}", Model.BreadcrumbTextSize, "px !important");
        }

        .beautyShop-big-title {
        font-size: @String.Format("{0}{1}", Model.BigTitleSize, "px !important");
        }

        .beautyShop-medium-title {
        font-size: @String.Format("{0}{1}", Model.MediumTitleSize, "px !important");
        }

        .beautyShop-small-title {
        font-size: @String.Format("{0}{1}", Model.SmallTitleSize, "px !important");
        }

        p{
        font-size: @String.Format("{0}{1}", Model.ParagraphTextSize, "px !important");
        }

        .breadcrumb-current-item h4{
        font-size: @String.Format("{0}{1}", Model.BreadcrumbTitleSize, "px !important");
        }

        .breadcrumb li *{
        font-size: @String.Format("{0}{1}", Model.BreadcrumbTextSize, "px !important");
        }

        .fieldset .title, .section .title {
        font-size: @String.Format("{0}{1}", Model.SmallTitleSize, "px !important");
        }